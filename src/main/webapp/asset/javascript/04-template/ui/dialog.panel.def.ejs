<div class="imethod-dialog <%= splitJoin(className) %>">
    <div class="imethod-dialog-heading">
        <h5 class="imethod-dialog-title">
            <%= title %>
        </h5>
    </div>
    <div class="imethod-dialog-body">
        <%= msg %>
    </div>
    <% if(!isEmptyArr(btns)){ %>
    <div class="imethod-dialog-footer">
        <%
            var l = btns.length;
        for(var i = 0;i < l;i++){
            var btn = btns[i];
        %>
        <div class="col-md-<%= 12 / l %>">
            <div class="btn btn-default <%= splitJoin(btn.className) %>"
                    <%= attr("id", btn.id) %>
                    <%= attr("name", btn.name) %>
                    <%= attr("value", btn.value) %>
                    >
                <%= btn.text %>
            </div>
        </div>
        <% } %>
    </div>
    <% } %>
</div>