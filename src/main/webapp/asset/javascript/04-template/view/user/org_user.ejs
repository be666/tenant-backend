<div>
    <form onsubmit="return false">
        <div class="row">
            <div class="col-md-2">
                用户编号
            </div>
            <div class="col-md-4">
                <input type="text" name="userCode" class="userCode" required/>
            </div>
            <div class="col-md-2">
                用户名
            </div>
            <div class="col-md-4">
                <input type="text" name="userName" class="userName" required/>
            </div>
        </div>
        <div class="row">
            <div class="col-md-2">
                性别
            </div>
            <div class="col-md-4">
                <i class="iMethod-radio man"></i>
                男
                <i class="iMethod-radio woman"></i>
                女
            </div>
            <div class="col-md-2">
                机构名称
            </div>
            <div class="col-md-4">
                <%= org['orgName'] %>
            </div>
        </div>
        <div class="row">
            <div class="col-md-2">
                电话
            </div>
            <div class="col-md-4">
                <input type="text" name="mobile" class="mobile" required/>
            </div>
            <div class="col-md-2">
                email
            </div>
            <div class="col-md-4">
                <input type="email" name="email" class="email" required/>
            </div>
        </div>
    </form>
</div>