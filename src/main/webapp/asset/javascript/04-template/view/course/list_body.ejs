<%
var titles = titles || [];
var tLength = titles.length;
var dataList = dataList || [];
var dLength = dataList.length;
var pk = pk || "";
for(var i = 0;i < dLength;i++){
    var data = dataList[i];
%>
<tr data-pk="<%= data[pk] || "" %>">
    <td class="iMethod-tlc">
        <i class="iMethod-check select-item"></i>
    </td>
    <% for(var j = 0;j < tLength;j++){
        var title = titles[j];
        var key = title['key'];
        var render = title['render'];
        var view = data[key];
        if (isFunc(render)) {
            view = render(data, data[key]);
        }
    %>
    <td>
        <%= view %>
    </td>
    <% } %>
    <td>
        <i class="iMethod-btn iMethod-btn-pri iMethod-btn-sm class-manager">班次管理</i>
    </td>
</tr>
<% } %>